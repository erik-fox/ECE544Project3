                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="Project3_MBlaze_Platform/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="Project3_MBlaze_Platform/standalone_domain/bspinclude/include"
                        sdx:bspConfig="Project3_MBlaze_Platform/standalone_domain/system.mss"
                />
