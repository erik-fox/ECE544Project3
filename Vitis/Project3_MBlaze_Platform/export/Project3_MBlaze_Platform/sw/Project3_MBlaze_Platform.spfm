<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="Project3_MBlaze_Platform"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
Project3_MBlaze_Platform
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="Project3_MBlaze_Platform">
        <sdx:configuration sdx:name="Project3_MBlaze_Platform"
                           sdx:displayName="Project3_MBlaze_Platform"
                           sdx:defaultProcessorGroup="standalone_domain"
